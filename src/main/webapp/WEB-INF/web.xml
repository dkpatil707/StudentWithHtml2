</servlet>
<servlet-mapping>
<servlet-name>sign</servlet-name>
<url-pattern>/signup</url-pattern>
</servlet-mapping>
  
  
  <servlet>
<servlet-name>log</servlet-name>
<servlet-class>studentwithhtmla2.controller.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>log</servlet-name>
